-- draw

function _draw()
 _drw()
	print_debug()
end

function drw_intro()
 cls(6)
 printc("sokoban",60,5)
 printc("by monsieurluge",66,5)
 printc("press üÖæÔ∏è to play",118,13)
end

function drw_game()
 cls(6)
 draw_stage()
 draw_player()
 draw_ui()
end

function drw_stage_selection()
 cls(6)
 for i=1,#stages do
  draw_stage_mini(i)
 end
 draw_selection()
end

function draw_player()
 spr(1,player.x*8+offset.x,player.y*8+offset.y) 
end

function draw_stage()
 map(0,0,offset.x,offset.y,stage.w,stage.h)
end

function draw_ui()
 print_info(stage.name)
 if stage.goal==0 then
  print("done!",2,2)
 end
end

function draw_selection()
 local s=stages[selection.nb]
 local x=selection.x
 local y=selection.y
 local dx=19*x-12
 local dy=19*y-15 
 rect(dx,dy,dx+16,dy+16,7)
 if s.lock then
  print_info("???")
 else
  print_info(s.name)
 end
end

function draw_stage_mini(nb)
	local s=stages[nb]
 local x=(nb-1)%6+1
 local y=flr((nb-1)/6)+1
 local dx=19*x-10
 local dy=19*y-13
 if s.lock then
  sspr(48,0,13,14,dx,dy,13,14)
  spr(38,dx+3,dy+3)
 elseif s.done then
  sspr(64,0,13,14,dx,dy,13,14)
 else
  sspr(48,0,13,14,dx,dy,13,14)
 end
end

function printc(txt,y,clr)
 local x=64-(#txt/2)*4
 print(txt,x,y,clr)
end

function print_debug()
 for i=1,#debug do
  local txt=debug[i]
  rectfill(1,i*8-4,#txt*3+6,i*8+2,2)
  print(txt,2,i*8-3,15)
 end
end

function print_info(txt)
 print(txt,3,121,13)
 print(txt,2,120,7)
end


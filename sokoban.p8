function _init()
 debug={}
 player={x=0,y=0}
 offset={x=0,y=0}
 selection={x=1,y=1,nb=1}
 _upd=upd_intro
 _drw=drw_intro
end

function load_stage(nb)
 stage=stages[nb]
 stage.goal=0
 stage.nb=nb
 offset.x=64-stage.w*4
 offset.y=64-stage.h*4
 for x=0,stage.w-1 do
  for y=0,stage.h-1 do
   local tile=mget(stage.x+x,stage.y+y)
   if tile==1 then
    player.x=x
    player.y=y
    mset(x,y,5)
   elseif tile==0 then
    mset(x,y,5)
   else
    mset(x,y,tile)
    if tile==4 then
     stage.goal+=1
    end
   end
  end
 end
end

function create_player()
 p={}
 p.x=0
 p.y=0
end


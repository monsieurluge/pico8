version 42
__lua__
-- sokoban
-- by monsieurluge

stages={
 {x=0,y=11,w=8,h=3,name="sokoban"},
 {x=0,y=17,w=9,h=7,name="the start"},
 {x=0,y=25,w=9,h=7,name="wtf?"},
}

function _init()
 player = {x=0,y=0}
 offset = {x=0,y=0}
 load_stage(1)
 _upd = upd_intro
 _drw = drw_intro
end

function load_stage(nb)
 stage = stages[nb]
 stage.goal = 0
 stage.nb = nb
 offset.x = 64-stage.w*4
 offset.y = 64-stage.h*4
 for x=0,stage.w-1 do
  for y=0,stage.h-1 do
   local tile = mget(stage.x+x,stage.y+y)
   if tile == 1 then
    player.x = x
    player.y = y
    mset(x,y,5)
   elseif tile == 0 then
    mset(x,y,5)
   else
    mset(x,y,tile)
    if tile == 4 then
     stage.goal += 1
    end
   end
  end
 end
end

function create_player()
 p = {}
 p.x = 0
 p.y = 0
end

-->8
-- update

function _update()
 _upd()
end

function upd_intro()
 if btnp(ðŸ…¾ï¸) then
  _upd=upd_game
  _drw=drw_game
 end
end

function upd_game()
 update_player()
 if stage.goal == 0 then
  load_stage(stage.nb+1)
 end
end

function add_box(x,y)
 local tile = tile_at(x,y)
 if tile.goal then
  mset(x,y,3)
  stage.goal -= 1
  sfx(1)
 else
  mset(x,y,2)
 end
end

function interact(dx,dy)
 local nx = player.x+dx
 local ny = player.y+dy
 local tile = tile_at(nx,ny)
 if tile.box and can_move(nx+dx,ny+dy) then
  remove_box(nx,ny,tile)
  add_box(nx+dx,ny+dy)
 end
end

function update_player()
 if (btnp() == 0) return
 local dx = 0
 local dy = 0
 if (btnp(â¬…ï¸)) dx = -1
 if (btnp(âž¡ï¸)) dx = 1
 if (btnp(â¬†ï¸)) dy = -1
 if (btnp(â¬‡ï¸)) dy = 1
 if (dx != 0 and dy != 0)
 or (dx == 0 and dy == 0) then
  return
 end
 local nx = player.x+dx
 local ny = player.y+dy
 if can_move(nx,ny) then
	 player.x = mid(0,nx,stage.w-1)
	 player.y = mid(0,ny,stage.h-1)
	 sfx(0)
	else
	 interact(dx,dy)
	end
end

function remove_box(x,y,tile)
 if tile.goal then
  mset(x,y,4)
  stage.goal += 1
  sfx(2)
 else
  mset(x,y,5)
 end
end

-->8
-- draw

function _draw()
 _drw()
end

function drw_intro()
 cls(6)
 printc("sokoban",60,5)
 printc("by monsieurluge",66,5)
 printc("press ðŸ…¾ï¸ to play",118,13)
end

function drw_game()
 cls(6)
 draw_stage()
 draw_player()
 draw_ui()
end

function draw_player()
 spr(1,player.x*8+offset.x,player.y*8+offset.y) 
end

function draw_stage()
 map(0,0,offset.x,offset.y,stage.w,stage.h)
end

function draw_ui()
 local infos=stage.name
 color(13)
 print(infos,3,121)
 color(7)
 print(infos,2,120)
 if stage.goal == 0 then
  print("done!",2,2)
 end
end

-->8
-- tools

function can_move(x,y)
 local tile = tile_at(x,y)
 return not tile.wall
  and not tile.box
end

function printc(txt,y,clr)
 local x = 64-(#txt/2)*4
 print(txt,x,y,clr)
end

function tile_at(x,y)
 local t = {}
 local sprite = mget(x,y)
 t.id = sprite
 t.box = fget(sprite,2)
 t.goal = fget(sprite,1)
 t.wall = fget(sprite,0)
 return t
end

